                    if (document.getElementById("input_GUIA").value != "") {
                        document.getElementById("input_DESTINO").focus();
                    }
                    else {
                        document.getElementById("input_GUIA").focus();
                    }

                    //$('form').submit();
                    break;
                    //esc
                case 27:
                    document.getElementById("input_CEDIS").focus();
                    break;
            }
        }

