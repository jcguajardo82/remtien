            //alert(keyCode);
            switch (keyCode) {
                //enter
                case 13:
                    switch (valueOption) {
                        case "1":
                            document.getElementById("formSalidaTransportista").submit();
                           // $('form').submit();

                            break;
                        case "2":
                            var ele = document.getElementById("Confirma");
                            if (ele.style.display == "block") {
                                ele.style.display = "none";
                                document.getElementById("input_MARCHAMO").focus();
                            }
                            else {
                                ele.style.display = "block";

                            }
                            var ele = document.getElementById("Seguro");
                            if (ele.style.display == "block") {
                                ele.style.display = "none";

                            }
                            else {
                                ele.style.display = "block";

                            }
                            break;
                        default:
                            document.getElementById("optionSelected3").focus();
                            break;
                    } break;
                    //esc
                case 27:
                    var ele = document.getElementById("Confirma");
                    if (ele.style.display == "block") {
                        ele.style.display = "none";
                        document.getElementById("input_MARCHAMO").focus();
