@model IEnumerable<EDReciboTiendaSoriana.EDReportes.EDRArtRemVen>
@{
    ViewBag.Title = "ArtRemVencidos";
    Layout = "~/Shared/Views/_Layoutsinmenu.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="row">
        <div class="col-lg-12">
            <br />
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox">
                <div class="ibox-content">
                    <h2>
                        <a href="@Url.Action("Index", "Menu")" class="btn btn-default btn-circle btn-lg"><i class="fa fa-reply"></i></a> Artículos de Remate Vencidos
                    </h2>
                    <div class="panel-body">
                        <div class="input-group-btn ">
                        </div>
                        <div class="row">
                            <div class="col-lg-3">
                                <div class="form-group">
                                    <label>Fecha : </label>
                                    <div class="input-group date" id="FechaArtRemVencidosDate">
                                        <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                        @Html.TextBox("FechaArtRemVencidos", null, new { @class = "form-control datepicker", @placeholder = "Fecha" })
                                    </div>
                                    <span class="label-danger error" style="visibility:collapse">Falta definir la Fecha</span>
                                </div>
                            </div>
                            <div class="col-lg-1">
                                <div class="form-group">
                                    <label>&nbsp;</label>
                                    <button class="btn btn-warning btn-artremven" type="button"><i class="fa fa-binoculars"></i>Buscar </button>
                                </div>
                            </div>
                            @*<div class="col-lg-3">
                                <div class="form-group">
                                    <label>Jefe de Departamento: </label>
                                    @Html.DropDownList("Id_Num_Dpto", (SelectList)ViewData["ddlSrcJefeDepto"], new { @class = "form-control" })
                                </div>
                            </div>*@
                            <div class="col-lg-3">                                
                            </div>
                            <div class="col-lg-3">
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-3">
                            </div>
                            <div class="col-lg-3">                                
                            </div>
                            <div class="col-lg-3">
                            </div>
                            
                        </div>
                        <div id="container">
                            <div id="divResults">
                                @{Html.RenderPartial("~/Views/Shared/_PartialRptArtRemVencidos.cshtml", Model);}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @section Styles {

        @Styles.Render("~/plugins/dataPickerStyles")
    }
    @section Scripts {
        @Scripts.Render("~/plugins/flot")
        @Scripts.Render("~/plugins/chartJs")
        @Scripts.Render("~/plugins/peity")
        @Scripts.Render("~/plugins/dataTables")
        @Scripts.Render("~/plugins/dataPicker")
        @Scripts.Render("~/plugins/slimScroll")
        @Scripts.Render("~/Scripts/RepArtRemVencidos")
    }

</div>
